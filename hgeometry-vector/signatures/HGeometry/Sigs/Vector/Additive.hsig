signature HGeometry.Sigs.Vector.Additive where

import HGeometry.Sigs.Vector
import HGeometry.Sigs.R

--------------------------------------------------------------------------------

-- | Zero Vector
zero :: Vector

-- | Apply a function to merge the 'non-zero' components of two
-- vectors, unioning the rest of the values.
liftU2 :: (R -> R -> R) -> Vector -> Vector -> Vector

 -- | Apply a function to the components of two vectors.
liftI2 :: (R -> R -> R) -> Vector -> Vector -> Vector
